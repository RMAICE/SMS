<nav class="flex max-w-7xl items-center justify-center p-6 lg:px-8">
  <div class="flex gap-x-3">
    {% for label, url in [
      ['Google', '/google/accounts'],
      ['Google сайты', '/google/sites'],
      ['Google stats', '/google/sites/analytics'],
      ['Binom', '/binom/accounts']
      ] %}
      {% if url == active_link %}
        <a href="{{ url }}"
           class="rounded-md bg-gray-900 px-3 py-2 text-sm font-medium text-white">{{ label }}</a>
      {% else %}
        <a href="{{ url }}"
           class="rounded-md px-3 py-2 text-sm font-medium text-gray-300 hover:bg-gray-700 hover:text-white">
          {{ label }}
        </a>
      {% endif %}
    {% endfor %}
  </div>
</nav>

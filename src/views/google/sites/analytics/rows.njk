{% from 'google/sites/analytics/date-select.njk' import dateSelect %}
{% for row in analytics %}
  <tr>
    <td class="whitespace-nowrap py-4 px-3 text-sm text-gray-900 font-medium">{{ row.url }}</td>
    <td class="whitespace-nowrap py-4 pl-3 text-sm text-gray-500 font-medium">{{ row.position_min }}</td>
    <td class="whitespace-nowrap py-4 pl-3 text-sm text-gray-500 font-medium">{{ row.position_max }}</td>
    <td class="whitespace-nowrap py-4 pl-3 text-sm text-gray-500 font-medium">{{ row.impressions }}</td>
    <td class="whitespace-nowrap py-4 pl-3 text-sm text-gray-500 font-medium">{{ row.clicks }}</td>
  </tr>
{% else %}
  <tr>
    <td colspan="4"
        class="whitespace-nowrap py-4 px-3 text-sm text-gray-900 font-medium">Нет отчета...</td>
  </tr>
{% endfor %}
{{ dateSelect(viewType, years, weeks, months, startDate, endDate) }}
